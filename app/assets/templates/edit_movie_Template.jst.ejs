<form id="editForm" enctype="multipart/form-data">
	<h1 style="font-weight: bold;">Edit Movie</h1>
	<table>
		<tr>
			<td><label style="font-size: large;"><b>Title:</b></label></td>
			
		</tr>
		<tr>
			<td><input type="text" id="title" /></td>
		</tr>
		<tr>
			<td><label style="font-size: large;"><b>Summary:</b></label></td>		
		</tr>
		<tr>
			<td><textarea type="text" id="summary" rows="5" cols="150"></textarea></td>
		</tr>
		<tr>
			<td><label style="font-size: large;"><b>Image:</b></label></td>		
		</tr>
		<tr>
			<td><input type="file" id="imageUpload" /></td>
		</tr>
		<tr height="20px"><td></td></tr>
		<tr>		
			<td>
				<input type="button" class="btn btn-primary" id='submit' value="Update Movie" />&nbsp;
				<input type="button" class="btn" id='cancel' value="Cancel" />
			</td>
		</tr>
	</table>
</form>